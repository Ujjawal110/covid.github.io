        <script>
            
            document.querySelector('form').addEventListener("submit", function(event){
  event.preventDefault();
    Login(username.value, password.value)
});

                invalid = document.querySelector('#invalid')
username = document.querySelector('#user__insta')
password = document.querySelector('#password__instagram')
maininsta = document.querySelector('#main__content__insta')                
          instamaincontent = document.querySelector('#App')    
           
            
                function empty(string) {
}

function Login(username, password) {
    if (username == 'null' && password == 'null' || password.length > 5){
        invalid.style.display = 'none'
        maininsta.style.display = 'none'
        instamaincontent.style.display = 'block'

    } else {
        if (empty(username) || empty(password)){
            invalid.textContent = 'PLEASE FILL USERNAME OR PASSWORD'
        } else {
            invalid.textContent = 'INVALID Username or Password '
        }
        invalid.style.display = 'block'
    }
}
            </script>
            
